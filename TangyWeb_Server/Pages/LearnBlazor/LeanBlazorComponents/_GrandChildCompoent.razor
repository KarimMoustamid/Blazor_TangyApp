<div class="row bg-warning my-4" style="background-color:#d9f99d">
    <h4 class="text-primary p-lg-4 pt-2 col-12">Grand Child Component</h4>
    <p>There is a message - @MessageForGrandChild , the lucky number is @LuckyNumber</p>
</div>

@code {
    [CascadingParameter(Name = "MessageForGrandChild")] public string MessageForGrandChild { get; set; }
    
    [CascadingParameter(Name = "LuckyNumber")] public string LuckyNumber { get; set; }
}